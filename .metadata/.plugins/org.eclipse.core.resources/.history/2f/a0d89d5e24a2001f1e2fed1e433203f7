package application.abstractClasses;

public abstract class Part {
	int id;
	String name;
	double price;
	int stock;
	int min;
	int max;
	Part(int id, String name, double price, int stock, int min, int max){
		this.id = id;
		this.name = name;
		this.price = price;
		this.stock = stock;
		this.min = min;
		this.max = max;
	}

	
}
